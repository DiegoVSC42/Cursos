package br.com.cursoJava.aulaFinal.teste;

public class PrimeiraClasse {
	public static void main(String args[]) {
		
		int numero = 3;
		int resultado = numero + 3;
		
		numero = numero + 3;
		numero += 3;
		numero -= 1;
		numero = numero/2;
		numero *= numero;
		
		numero ++;
		numero --;
		
		float numero1 = 10;
		double numero2 = 10;
		
	}
}
