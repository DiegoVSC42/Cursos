package Geometria;

public class Quadrado implements FiguraGeometrica{
	
	double comprimento;
	double largura;
	@Override
	public double area() {
		double valorArea = comprimento*largura;
		return valorArea;
	}

	@Override
	public double perimetro() {
		// TODO Auto-generated method stub
		return 0;
	}
	
	

}
